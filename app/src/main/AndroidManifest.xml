<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.kingofservice">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KingOfService">
        <activity android:name=".TelaPerfilPrestador"></activity>
        <activity android:name=".TelaListagem" />
        <activity android:name=".TelaFeed" />
        <activity
            android:name=".TelaCadastroPrestador"
            android:parentActivityName=".TelaTipoUsuario" />
        <activity
            android:name=".TelaCadastroCliente"
            android:parentActivityName=".TelaTipoUsuario" />
        <activity
            android:name=".TelaTipoUsuario"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".TelaLogin"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>