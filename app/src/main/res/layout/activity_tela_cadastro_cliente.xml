<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="409dp"
    android:layout_height="729dp"
    tools:layout_editor_absoluteX="dp"
    tools:layout_editor_absoluteY="1dp"
    android:fillViewport="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".TelaCadastroCliente">

        <androidx.cardview.widget.CardView
            android:id="@+id/card_view_profile"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:foregroundGravity="center"
            app:cardCornerRadius="45dp"
            android:layout_marginTop="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/iv_profile"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:contentDescription="TODO"
                android:scaleType="centerCrop"
                android:src="@drawable/profile" />

        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/tv_trocar_foto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto"
            android:text="Trocar foto"
            android:textColor="@color/colorPrimary"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/card_view_profile" />

        <TextView
            android:id="@+id/tv_nomeCadastroCliente"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto"
            android:text="Nome"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/et_nomeCadastroCliente"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.161"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_trocar_foto"
            app:layout_constraintVertical_bias="0.463" />

        <TextView
            android:id="@+id/tv_dataNascimentoCadastroCliente"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/roboto"
            android:text="Data Nascimento"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_nomeCadastroCliente" />

        <TextView
            android:id="@+id/tv_emailCadastroCliente"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/roboto"
            android:text="E-mail"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.161"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_dataNascimentoCadastroCliente" />

        <TextView
            android:id="@+id/tv_senhaCadastroCliente"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/roboto"
            android:text="Senha"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.162"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_emailCadastroCliente" />

        <TextView
            android:id="@+id/tv_generoCadastroCliente"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/roboto"
            android:text="GenÃªro"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.162"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_senhaCadastroCliente" />

        <TextView
            android:id="@+id/tv_cepCadastroCliente"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/roboto"
            android:text="CEP"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.156"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/radioGroup_cadastroCliente"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/tv_estadoCadastroCliente"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/roboto"
            android:text="Estado"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/et_estadoCadastroCliente"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.161"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_cepCadastroCliente"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/et_nomeCadastroCliente"
            android:layout_width="300dp"
            android:layout_height="44dp"
            android:layout_marginTop="60dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/name_24"
            android:paddingLeft="10dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_nomeCadastroCliente"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_nomeCadastroCliente"
            app:layout_constraintTop_toBottomOf="@+id/tv_trocar_foto" />

        <EditText
            android:id="@+id/et_dataNascimentoCadastroCliente"
            android:layout_width="300dp"
            android:layout_height="44dp"
            android:layout_marginTop="56dp"
            android:paddingLeft="10dp"
            android:drawableStart="@drawable/calendario_24"
            android:background="@drawable/edit_text_login_background"
            app:layout_constraintEnd_toEndOf="@+id/tv_dataNascimentoCadastroCliente"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_dataNascimentoCadastroCliente"
            app:layout_constraintTop_toBottomOf="@+id/et_nomeCadastroCliente" />

        <EditText
            android:id="@+id/et_emailCadastroCliente"
            android:layout_width="300dp"
            android:layout_height="44dp"
            android:layout_marginTop="52dp"
            android:inputType="textEmailAddress"
            android:paddingLeft="10dp"
            android:drawableStart="@drawable/email_24"
            android:background="@drawable/edit_text_login_background"
            app:layout_constraintEnd_toEndOf="@+id/tv_emailCadastroPrestador"
            app:layout_constraintHorizontal_bias="0.003"
            app:layout_constraintStart_toStartOf="@+id/tv_emailCadastroCliente"
            app:layout_constraintTop_toBottomOf="@+id/et_dataNascimentoCadastroCliente" />

        <EditText
            android:id="@+id/et_senhaCadastroCliente"
            android:layout_width="300dp"
            android:layout_height="44dp"
            android:layout_marginTop="52dp"
            android:inputType="textPassword"
            android:paddingLeft="10dp"
            android:drawableStart="@drawable/lock_24"
            android:background="@drawable/edit_text_login_background"
            app:layout_constraintEnd_toEndOf="@+id/tv_senhaCadastroCliente"
            app:layout_constraintHorizontal_bias="0.003"
            app:layout_constraintStart_toStartOf="@+id/tv_senhaCadastroCliente"
            app:layout_constraintTop_toBottomOf="@+id/et_emailCadastroCliente" />

        <EditText
            android:id="@+id/et_cepCadastroCliente"
            android:layout_width="300dp"
            android:layout_height="44dp"
            android:layout_marginTop="52dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingLeft="10dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_cepCadastroCliente"
            app:layout_constraintHorizontal_bias="0.003"
            app:layout_constraintStart_toStartOf="@+id/tv_cepCadastroCliente"
            app:layout_constraintTop_toBottomOf="@+id/radioGroup_cadastroCliente"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/et_estadoCadastroCliente"
            android:layout_width="300dp"
            android:layout_height="44dp"
            android:layout_marginTop="56dp"
            android:paddingLeft="10dp"
            android:drawableStart="@drawable/localizacao_24"
            android:background="@drawable/edit_text_login_background"
            app:layout_constraintEnd_toEndOf="@+id/tv_estadoCadastroCliente"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_estadoCadastroCliente"
            app:layout_constraintTop_toBottomOf="@+id/et_cepCadastroCliente" />

        <TextView
            android:id="@+id/tv_cidadeCadastroCliente"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/roboto"
            android:text="Cidade"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/et_cidadeCadastroCliente"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.164"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_estadoCadastroCliente"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tv_bairroCadastroCliente"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/roboto"
            android:text="Bairro"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/et_bairroCadastroCliente"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.164"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_cidadeCadastroCliente"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tv_ruaCadastroCliente"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/roboto"
            android:text="Rua"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/et_ruaCadastroCliente"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.161"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_bairroCadastroCliente"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tv_numeroCadastroCliente"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/roboto"
            android:text="Numero"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/et_numeroCadastroCliente"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.164"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_ruaCadastroCliente"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tv_complementoCadastroCliente"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/roboto"
            android:text="Complemento"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/et_complementoCadastroCliente"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.183"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_numeroCadastroCliente"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/et_cidadeCadastroCliente"
            android:layout_width="300dp"
            android:layout_height="44dp"
            android:layout_marginTop="56dp"
            android:paddingLeft="10dp"
            android:drawableStart="@drawable/localizacao_24"
            android:background="@drawable/edit_text_login_background"
            app:layout_constraintEnd_toEndOf="@+id/tv_cidadeCadastroCliente"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_cidadeCadastroCliente"
            app:layout_constraintTop_toBottomOf="@+id/et_estadoCadastroCliente" />

        <EditText
            android:id="@+id/et_bairroCadastroCliente"
            android:layout_width="300dp"
            android:layout_height="44dp"
            android:layout_marginTop="56dp"
            android:paddingLeft="10dp"
            android:drawableStart="@drawable/localizacao_24"
            android:background="@drawable/edit_text_login_background"
            app:layout_constraintEnd_toEndOf="@+id/tv_bairroCadastroCliente"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_bairroCadastroCliente"
            app:layout_constraintTop_toBottomOf="@+id/et_cidadeCadastroCliente" />

        <EditText
            android:id="@+id/et_ruaCadastroCliente"
            android:layout_width="300dp"
            android:layout_height="44dp"
            android:layout_marginTop="48dp"
            android:paddingLeft="10dp"
            android:drawableStart="@drawable/localizacao_24"
            android:background="@drawable/edit_text_login_background"
            app:layout_constraintEnd_toEndOf="@+id/tv_ruaCadastroCliente"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_ruaCadastroCliente"
            app:layout_constraintTop_toBottomOf="@+id/et_bairroCadastroCliente" />

        <EditText
            android:id="@+id/et_numeroCadastroCliente"
            android:layout_width="300dp"
            android:layout_height="44dp"
            android:layout_marginTop="60dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingLeft="10dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_numeroCadastroCliente"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_numeroCadastroCliente"
            app:layout_constraintTop_toBottomOf="@+id/et_ruaCadastroCliente" />

        <EditText
            android:id="@+id/et_complementoCadastroCliente"
            android:layout_width="300dp"
            android:layout_height="44dp"
            android:layout_marginTop="56dp"
            android:layout_marginBottom="55dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingLeft="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/tv_complementoCadastroCliente"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_complementoCadastroCliente"
            app:layout_constraintTop_toBottomOf="@+id/et_numeroCadastroCliente" />

        <RadioGroup
            android:id="@+id/radioGroup_cadastroCliente"
            android:layout_width="333dp"
            android:layout_height="47dp"
            android:layout_marginTop="52dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="@+id/tv_generoCadastroCliente"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_generoCadastroCliente"
            app:layout_constraintTop_toBottomOf="@+id/et_senhaCadastroCliente">

            <RadioButton
                android:id="@+id/radio_masculino"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="@color/colorPrimaryDark"
                android:drawablePadding="8dp"
                android:drawableTint="@color/colorPrimary"
                android:text="Masculino" />

            <RadioButton
                android:id="@+id/radio_feminino"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="@color/colorPrimaryDark"
                android:drawablePadding="8dp"
                android:drawableTint="@color/colorPrimary"
                android:text="Feminino" />
        </RadioGroup>

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>