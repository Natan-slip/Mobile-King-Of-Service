<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="409dp"
    android:layout_height="729dp"
    tools:layout_editor_absoluteX="dp"
    tools:layout_editor_absoluteY="1dp"
    android:fillViewport="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".TelaCadastroPrestador">

        <androidx.cardview.widget.CardView
            android:id="@+id/card_view_profile"
            android:layout_width="100dp"
            android:layout_height="100dp"
            app:cardCornerRadius="45dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/iv_profile"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:contentDescription="TODO"
                android:scaleType="centerCrop"
                android:src="@drawable/profile" />

        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/tv_trocar_foto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto"
            android:text="Trocar foto"
            android:textColor="@color/colorPrimary"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/card_view_profile" />

        <TextView
            android:id="@+id/tv_nomeCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto"
            android:text="Nome"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/et_nomeCadastroPrestador"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.091"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_trocar_foto" />

        <EditText
            android:id="@+id/et_nomeCadastroPrestador"
            android:layout_width="341dp"
            android:layout_height="46dp"
            android:layout_marginTop="60dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/name_24"
            android:paddingLeft="10dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_nomeCadastroPrestador"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_nomeCadastroPrestador"
            app:layout_constraintTop_toBottomOf="@+id/tv_trocar_foto" />

        <TextView
            android:id="@+id/tv_dataNascimentoCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="Data Nascimento"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.109"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_nomeCadastroPrestador" />

        <EditText
            android:id="@+id/et_dataNascimentoCadastroPrestador"
            android:layout_width="341dp"
            android:layout_height="46dp"
            android:layout_marginTop="56dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/calendario_24"
            android:paddingLeft="10dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_dataNascimentoCadastroPrestador"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_dataNascimentoCadastroPrestador"
            app:layout_constraintTop_toBottomOf="@+id/et_nomeCadastroPrestador" />

        <TextView
            android:id="@+id/tv_emailCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="E-mail"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.091"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_dataNascimentoCadastroPrestador" />

        <EditText
            android:id="@+id/et_emailCadastroPrestador"
            android:layout_width="341dp"
            android:layout_height="46dp"
            android:layout_marginTop="52dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/email_24"
            android:paddingLeft="10dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_emailCadastroPrestador"
            app:layout_constraintHorizontal_bias="0.003"
            app:layout_constraintStart_toStartOf="@+id/tv_emailCadastroPrestador"
            app:layout_constraintTop_toBottomOf="@+id/et_dataNascimentoCadastroPrestador" />

        <TextView
            android:id="@+id/tv_senhaCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="Senha"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.091"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_emailCadastroPrestador" />

        <EditText
            android:id="@+id/et_senhaCadastroPrestador"
            android:layout_width="341dp"
            android:layout_height="46dp"
            android:layout_marginTop="52dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/lock_24"
            android:paddingLeft="10dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_senhaCadastroPrestador"
            app:layout_constraintHorizontal_bias="0.003"
            app:layout_constraintStart_toStartOf="@+id/tv_senhaCadastroPrestador"
            app:layout_constraintTop_toBottomOf="@+id/et_emailCadastroPrestador" />

        <TextView
            android:id="@+id/tv_generoCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="Genêro"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.09"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_senhaCadastroPrestador" />

        <TextView
            android:id="@+id/tv_cepCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="CEP"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.088"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/radioGroup_cadastroPrestador"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/et_cepCadastroPrestador"
            android:layout_width="341dp"
            android:layout_height="46dp"
            android:layout_marginTop="52dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingLeft="10dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_cepCadastroPrestador"
            app:layout_constraintHorizontal_bias="0.003"
            app:layout_constraintStart_toStartOf="@+id/tv_cepCadastroPrestador"
            app:layout_constraintTop_toBottomOf="@+id/radioGroup_cadastroPrestador"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/tv_estadoCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="Estado"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/et_estadoCadastroPrestador"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.09"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_cepCadastroPrestador"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/et_estadoCadastroPrestador"
            android:layout_width="341dp"
            android:layout_height="46dp"
            android:layout_marginTop="56dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingLeft="10dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_estadoCadastroPrestador"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_estadoCadastroPrestador"
            app:layout_constraintTop_toBottomOf="@+id/et_cepCadastroPrestador" />

        <TextView
            android:id="@+id/tv_cidadeCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="Cidade"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/et_cidadeCadastroPrestador"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.09"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_estadoCadastroPrestador"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/et_cidadeCadastroPrestador"
            android:layout_width="341dp"
            android:layout_height="46dp"
            android:layout_marginTop="56dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingLeft="10dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_cidadeCadastroPrestador"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_cidadeCadastroPrestador"
            app:layout_constraintTop_toBottomOf="@+id/et_estadoCadastroPrestador" />

        <TextView
            android:id="@+id/tv_bairroCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="Bairro"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/et_bairroCadastroPrestador"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.09"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_cidadeCadastroPrestador"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/et_bairroCadastroPrestador"
            android:layout_width="341dp"
            android:layout_height="46dp"
            android:layout_marginTop="56dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingLeft="10dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_bairroCadastroPrestador"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_bairroCadastroPrestador"
            app:layout_constraintTop_toBottomOf="@+id/et_cidadeCadastroPrestador" />

        <TextView
            android:id="@+id/tv_ruaCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="Rua"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/et_ruaCadastroPrestador"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.09"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_bairroCadastroPrestador"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/et_ruaCadastroPrestador"
            android:layout_width="341dp"
            android:layout_height="46dp"
            android:layout_marginTop="48dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingLeft="10dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_ruaCadastroPrestador"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_ruaCadastroPrestador"
            app:layout_constraintTop_toBottomOf="@+id/et_bairroCadastroPrestador" />

        <TextView
            android:id="@+id/tv_numeroCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="Numero"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/et_numeroCadastroPrestador"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.09"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_ruaCadastroPrestador"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/et_numeroCadastroPrestador"
            android:layout_width="341dp"
            android:layout_height="46dp"
            android:layout_marginTop="56dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingLeft="10dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_numeroCadastroPrestador"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_numeroCadastroPrestador"
            app:layout_constraintTop_toBottomOf="@+id/et_ruaCadastroPrestador" />

        <EditText
            android:id="@+id/et_complementoCadastroPrestador"
            android:layout_width="341dp"
            android:layout_height="46dp"
            android:layout_marginTop="56dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingLeft="10dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_complementoCadastroPrestador"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_complementoCadastroPrestador"
            app:layout_constraintTop_toBottomOf="@+id/et_numeroCadastroPrestador" />

        <TextView
            android:id="@+id/tv_complementoCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="Complemento"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/et_complementoCadastroPrestador"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.099"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_numeroCadastroPrestador"
            app:layout_constraintVertical_bias="0.136" />

        <TextView
            android:id="@+id/tv_profissaoCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="Profissão"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/et_profissaoCadastroPrestador"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.102"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_complementoCadastroPrestador"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/et_profissaoCadastroPrestador"
            android:layout_width="341dp"
            android:layout_height="46dp"
            android:layout_marginTop="56dp"
            android:layout_marginBottom="60dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingLeft="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/tv_profissaoCadastroPrestador"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_profissaoCadastroPrestador"
            app:layout_constraintTop_toBottomOf="@+id/et_complementoCadastroPrestador"
            app:layout_constraintVertical_bias="1.0" />

        <RadioGroup
            android:id="@+id/radioGroup_cadastroPrestador"
            android:layout_width="333dp"
            android:layout_height="47dp"
            android:layout_marginTop="52dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="@+id/tv_generoCadastroPrestador"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_generoCadastroPrestador"
            app:layout_constraintTop_toBottomOf="@+id/et_senhaCadastroPrestador">

            <RadioButton
                android:id="@+id/radio_masculino"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="@color/colorPrimaryDark"
                android:drawablePadding="8dp"
                android:drawableTint="@color/colorPrimary"
                android:text="Masculino" />

            <RadioButton
                android:id="@+id/radio_feminino"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="@color/colorPrimaryDark"
                android:drawablePadding="8dp"
                android:drawableTint="@color/colorPrimary"
                android:text="Feminino" />
        </RadioGroup>

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>